<div class="row">
  <div class="dashboard-search">
    <h5 class="col 12"><i class="fa fa-pie-chart fa-lg" aria-hidden="true"></i> Tickets Report <small>(<%= @event.title %>)</small></h5>
  </div>
</div>
<div class="row">
    <div class="col s12">
      <ul class="tabs">
        <li class="tab col s3"><a href="#ticketSummary" class="active">Summary</a></li>
        <li class="tab col s3"><a href="#allBookings" class="active">All Bookings</a></li>
        <li class="tab col s3"><a href="#eventAttendees">Attendees</a></li>
      </ul>
    </div>
    <div id="ticketSummary" class="col s12">
      <%= render partial: "bookings/partials/tickets_summary"%>
    </div>
    <div id="allBookings" class="col s12">
        <%= render partial: "bookings/partials/all_bookings"%>
    </div>
    <div id="eventAttendees" class="col s12">
        <%= render partial: "bookings/partials/attendees"%>
    </div>
  </div>
</div>
