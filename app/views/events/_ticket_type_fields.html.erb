<div class="row col s12 nested-fields " id="paid_ticket_div">
  <div class="input-field col s5">
    <i class="fa fa-ticket prefix icon-setting"></i>
    <%= f.text_field :name, placeholder: "Ticket Title" %>
  </div>
  <div class="input-field col s2">
    <%= f.text_field :quantity, min: 0, placeholder: "Quantity" %>
  </div>
  <div class="input-field col s2">
    <%= f.text_field :price, min: 0, placeholder: "Price"%>
  </div>
  <div class="col s2">
    <%= link_to_remove_association "x", f, class: "btn-floating btn waves-effect waves-light red"  %>
  </div>
</div>
