<div class="row col s12" id="paid_ticket_div">
  <div class="input-field col s5">
    <i class="fa fa-ticket prefix icon-setting"></i>
    <%= f.label :name, "Ticket Title" %>
    <%= f.text_field :name, placeholder: "Ticket Title" %>
  </div>
  <div class="input-field col s2">
    <%= f.label :quantity, "Quantity" %>
    <%= f.text_field :quantity, min: 0, placeholder: "Quantity" %>
  </div>
  <div class="input-field col s2">
    <%= f.label :price, "Price" %>
    <%= f.text_field :price, min: 0, placeholder: "Price"%>
  </div>
</div>
