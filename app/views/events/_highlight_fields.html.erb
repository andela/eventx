<div class="row col s12 hide highlight_fields" id = "highlight_field<%= field_id %>">
  <div class="input-field col s5">
    <%= f.text_field :title, placeholder: "Title" , class: "highlight_title", id: "title" %>
  </div>
  <div class="input-field col s10">
    <%= f.text_area :description, rows: 6, placeholder: "Enter Highlight Description", class: "highlight_description"%>
  </div>
  <div class="input-field col s5">
    <%= f.text_field :start_time, class: "timepicker highlight_start_time", placeholder: "Start Time" %>
  </div>
  <div class="input-field col s5">
    <%= f.text_field :end_time, class: "timepicker highlight_end_time", placeholder: "End Time" %>
  </div>
  <div class="input-field col s5">
    <%= f.text_field :image_title, placeholder: "Caption" , class: "highlight_caption"%>
  </div>
  <div class="input-field col s5">
    <%= f.file_field :image,  class: "highlight_photo_upload", id: "highlight_photo_upload" %>
  </div>
</div>
