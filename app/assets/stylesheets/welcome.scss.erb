html, body {
  height: 100%;
  width: 100%;
}

a {
  color: #4ABDAC;
}

.parallax-container{
  height: 650px;
  width: 100%;
  background-color: rgba(0,0,0,0.45);
}

#event {
  position: absolute;
  height: inherit;
  width: 100%;
  text-align: center;
  margin: auto;
  background-color: rgba(0,0,0,0.4);
}

.home_app_title {
  color: #fff !important;
  text-shadow: 2px 1px 1px black;
  font-family: 'Roboto', cursive;
  margin:0;
  padding: 25px 0;
  font-size: 3rem;
}

.home_user_options {
  text-align: right;
}

.home_user_options .home_sign_in_or_up {
  display: inline;
}

.home_user_options .home_sign_in_or_up .btn_a_tag {
  text-decoration: none;
  color: #fff;
}

.home_app_tag {
  width: 20%;
  font-size: 1.5rem;
  color: #fff;
  margin: auto;
}

.btn-large.home_button {
  font-size: 1.25em;
  border-style: solid #EED8AE;
  border-radius: 6px;
  background-color: #00897b;
  color: #ffffff
}

.home_tag {
  position: relative;
  top: 12%;
  bottom: 12%;
}

.search_form {
  position: relative;
  background-color: rgba(0, 0, 0, 0.64);
  bottom: 0 !important;

  select {
    width: 80%;
    margin-left: 20%;
    background-color: rgba(255, 255, 255, 0);
    border: none;
    border-bottom: 1px solid #fff;
    -webkit-appearance: none !important;
    -webkit-border-radius: 0px !important;
    -moz-border-radius: 0px !important;
    -moz-appearance: none !important;
    text-decoration: none !important;
    border-radius: none !important;
    &:focus {
      outline: none;
    }
  }

}

.white_text{
  color: #fff;
}


.black_text{
  color: black;
}

.red_text{
  color: #F44336;
}

.orange_text {
  color: #FC4A1A;
}

.yellow_text {
  color: #F7B733;
}

.blue_text{
  color: #2196F3;
}

.teal_text{
  color: #4ABDAC;
}

input[type=text]:focus:not([readonly]) {
  border-bottom: 1px solid #fff;
  box-shadow: 0 1px 0 0 #EED8AE;
}

.content-set {
  margin: 40px;
}

.event-section {
  background:#F7F7F7 ;
 }

.card-shadow {
  box-shadow: 0 1px 0 0 rgba(0,0,0,.1);
}

.card .card-content .card-title {
  line-height: 0px;
  width: 15px;
}

.card-title {
  font-size: 15px;
  margin-top: 0;
  margin-bottom: 30px;
  font-weight: 300;
  letter-spacing: 1px;
}

.card .card-action {
  border-top: 1px solid rgba(160, 160, 160, 0.2);
  padding: 10px 20px;
}

.pointer {
  cursor: pointer;
}

.Amount {
  text-transform: uppercase;
  background-color: #EED8AE;
  font-size: 12px;
  line-height: 20px;
  color: #222;
  box-shadow: 2px 2px 2px #000;
  box-sizing: border-box;
  padding: 2px 10px;
  position: absolute;
  display: inline-block;
  top: 10px;
  right: 10px;
  z-index: 50;
}

.card .card-reveal {
  padding: 20px;
  position: absolute;
  background-color: #fff;
  width: 100%;
  overflow-y: auto;
  top: 100%;
  height: 100%;
  z-index: 51;
  display: none;
}

.remove-margin {
  margin: 0;
}

.tabs .indicator {
  position: absolute;
  bottom: 0;
  height: 2px;
  background-color: #FFE0A6;
  will-change: left, right;
}

.tabs .tab a {
  color: rgb(140, 131, 131);
  display: block;
  width: 100%;
  height: 100%;
  font-weight: bolder;
  -webkit-transition: color 0.28s ease;
  -moz-transition: color 0.28s ease;
  -o-transition: color 0.28s ease;
  -ms-transition: color 0.28s ease;
  transition: color 0.28s ease;
}

.tabs .tab a:hover {
  color: #f9c9cb;
}

footer.page-footer {
  position: relative;
  z-index: 40;
  margin-top: 0px;
  padding-top: 20px;
  background-color: #333333;
  border-top: 5px solid #666666;
  a {
    color: #666;
  }
  nav {
    color: #fff;
    background-color: #333333;
    width: 100%;
    height: 56px;
    line-height: 56px;
    border-bottom: #000000 2px solid;
  }
}


.hideme {
  opacity:0;
}

i.when_icon {
  color: white;
  position: absolute;
  left: 5px;
}

.btn-large.bottom_btn {
  background-color: #EED8AE;
  color: #000000
}

nav.custom_nav {
  background-color: rgba(0, 0, 0, 0.0);
  width: 100%;
  color: white !important;
  top: 0;
  position: absolute;
}

.sso_option .btn {
  width: 100% !important;
}

.profile_pic.circle {
    vertical-align: middle;
    width: 50px;
    height: 50px;
    border: solid 3px rgba(255,255,255, 0.6);
}

.our-center {
  margin:0 auto !important;
}

.alert-scroll-under {
  display: inline-block;
  vertical-align: middle;
  height: 2%;
  width: 2%;
  background-position: 0 -1865px;
  background-repeat: no-repeat;
  position: absolute;
  z-index: 15;
  bottom: 2.625rem;
  left: 50%;
  margin-left: -0.375rem;
}

.drop {
  color: #6B5936;
}

.slider .slides li .caption {
  color: #26a69a;
  position: absolute;
  top: 15%;
  left: 15%;
  width: 70%;
  opacity: 0.50;
}

.slid-box {
  font-size: 40px !important;
  text-transform: capitalize;
  margin: 1.46rem 0 1.168rem 0;
  background: #fff;
  width: 100%;
  opacity: 0.80;
}

.slid-desc-box {
  font-size: 1.62rem;
  text-transform: capitalize;
  background: #26a69a;
  width: 70%;
  margin-left: 30%;
  color: #fff;
  opacity: 0.90;
}

.remove-position {
  color: #9e9e9e;
  position: relative !important;
  top: 0.02rem !important;
  left: 0.75rem;
  font-size: 1rem;
  cursor: text;
}

.transform-text {
  text-transform: capitalize !important;
}

a.remove-text {
  color: #ffffff !important;
}

.about-top {
  position: relative;
  background-image: url("https://pixabay.com/static/uploads/photo/2014/09/07/17/41/girl-438129_960_720.jpg");
  background-size: 120%;
  padding: 20px;
  height: 350px;
}

.about-triangle {
  background: #4abdac;
  height: 350px;
}

.left-triangle {
  height: 100px;
  display: inline-block;
  float: left;
  width: 50%;
  border-right: 50vw solid transparent;
  border-bottom: 100px solid #4abdac;
}

.divider {
  background-color: rgba(0, 128, 128, 0.6);
  width: 100%;
  height: 100px;
  display: inline-block;
  float: left;
  width: 50%;
  border-left: 50vw solid transparent;
  border-bottom: 100px solid #4abdac;
}

.right-left-triangle {
  position: relative;
  top: -100px;
}

.about {
  background: #4abdac;
  height: 250px;
}

.about-header-content {
  width: 70%;
  margin: auto;
  font-size: 65px;
  font-weight: 200;
  color: #fff;
  position: relative;
  top: 60px;
  text-align: center;
}

@media only screen and (max-width : 992px) {
  .about-header-content {
    font-size: 40px;
  }
}

.about {
  padding: 75px;
}

.about-content {
  padding-bottom: 70px;
  margin: auto;
  font-size: 1.5em;
  font-weight: 200;
}

.about-content h4 {
  padding: 10px;
  padding-left: 0
}

.no-margin {
  display: inline-block;
  float: left;
  width: 25%;
}

.stat-group {
  margin-top: 1rem
}

.about-stats {
  height: 175px;
  width: 95%;
  padding: 0 30px;
  margin: auto;
}

nav ul a:hover {
  background-color: rgba(74, 189, 172, 0.4);
  font-size: 1.03rem;
  font-weight: 500;
  margin: auto;
}

.about-stats-div {
  background: #e3efef;
}

.row.card-group {
    padding: 0 30px;
    margin-top: 200px;
    margin-bottom: 200px;
}

.card .card-image img {
  height: 250px;
}

.col.padding-side-25 {
  padding: 0 35px;
}

.col.padding-side-25 p {
  font-size: 18px;
}

.justify-align {
  text-align: justify;
}

.col.padding-side-25 h4 {
  font-weight: 300;
  text-align: center;
}

.milestones {
  padding: 25px 10px 10px;
  color: #fff;
  text-align: center;
  border-radius: 2px;
  background: #4ABDAC;
  -webkit-box-shadow: 0 8px 6px -6px black;
  -moz-box-shadow: 0 8px 6px -6px black;
  box-shadow: 0 8px 6px -6px black;
}

.milestones .stats-large {
  font-size: 40px;
  font-weight: 300;
}

.feature-details {
  position: relative;
  bottom: 102px;
  color: #fff;
  padding: 0 10px 0 10px;
  background: rgba(0, 0,0, 0.5);
}

.feature-title {
  font-size: 25px;
  font-weight: 300;
}

.card.feature-card {
  margin-left: -12px;
  margin-right: -10px;
}

#faq-row {
  background: url(/assets/nearfooter.png);
  padding: 20px 0 20px;
  margin: 9px 0 0px;
}

#faq {
  width: 65%;
  margin: auto;
}
