  }
}

.ticket_type{
  display: inline-block;
  transform: rotate(45deg);
  position: absolute;
  right: -5px;
  background: goldenrod none repeat scroll 0% 0%;
  top: 25px;
  width: 80px;
  text-align: center;
}

.medium .card-content p a, .medium .card-content p{
  color: white;
  font-size: small !important;
}

