td, th {
  padding: 15px 5px;
  display: table-cell;
  text-align: -webkit-center;
  vertical-align: middle;
  border-radius: 2px;
}

.manage_btn {
    border: none;
    border-radius: 2px;
    display: inline-block;
    height: 36px;
    outline: 0;
    padding: 0 1rem;
    text-transform: capitalize;
    vertical-align: middle;
    background-color: #00695c;
    color: #fff;
    font-size: 12px;
    -webkit-tap-highlight-color: transparent;
}
.post {
      margin-top: 40px;
    margin-left: 45%;
}

.col.s6.offset-s4.post {
    margin-left: 45%;
}

.near-footer {
    border-top: 1px solid #dedede;
    position: relative;
    max-width: 100%;
    padding-top: 20px;
    padding-bottom: 20px;
    background-image: url('<%= asset_path('nearfooter.png') %>');
    text-align: center;
    p{
        display: block;
        line-height: 30px;
        text-align: center;
        width: 50%;
        margin: 0px auto;
        color: #666666;
        font-size: 17px;
    }
    a{
        margin: 30px 0px;
    }

}


