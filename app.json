  },
  "env":{
    "app_host":{
      "required":true
    },
    "FACEBOOK_APP_ID":{
      "required":true
    },
    "FACEBOOK_SECRET":{
      "required":true
    },
    "GITHUB_KEY":{
      "required":true
    },
    "GITHUB_SECRET":{
      "required":true
    },
    "GOOGLE_CLIENT_ID":{
      "required":true
    },
    "GOOGLE_CLIENT_SECRET":{
